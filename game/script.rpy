transform load_trans(Begin,Continue,After):
    alpha 0
    5
    block:
        pos(1700,1000)
        alpha 0
        pause Begin
        alpha 1
        pause Continue
        alpha 0
        pause After
        repeat

transform load_stay():
    pos(1700,1000)
    alpha 0
    3
    linear 1.5 alpha 1

screen ctc(arg=None):
    zorder 105
    add "load_1" at load_trans(0.0,1.0,2.0)
    add "load_2" at load_trans(1.0,0.35,1.65)
    add "load_3" at load_trans(1.35,0.3,1.35)
    add "load_4" at load_trans(1.65,0.25,1.1)
    add "load_5" at load_trans(1.9,0.2,0.9)
    add "load_6" at load_trans(2.1,0.25,0.65)
    add "load_7" at load_trans(2.35,0.3,0.35)
    add "load_8" at load_trans(2.65,0.35,0.0)
    add "load_0" at load_stay()

label chapter_trans:
    scene chapter_tr1
    with fade
    pause(0.5)
    show chapter_tr2
    with dissolve
    pause(0.5)
    show chapter_tr3
    with mytrans1
    pause 2
    return


label splashscreen:
    scene white
    show logo:
        xpos 480
        ypos 270
    with dissolve
    pause(1)
    hide logo
    with dissolve
    return

