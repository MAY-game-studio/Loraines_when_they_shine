image chapter_tr1:
    contains:
        "images/转场/背景.png"
        size(1920,1080)
image chapter_tr2:
    contains:
        "images/转场/诺琪小人.png"
        size(1920,1080)
image chapter_tr3:
    contains:
        "images/转场/文字.png"
        size(1920,1080)

label chapter_trans:
    scene chapter_tr1
    with fade
    pause(0.5)
    show chapter_tr2
    with dissolve
    pause(0.5)
    show chapter_tr3
    with mytrans1
    pause
    return


label splashscreen:
    scene white
    show logo:
        xpos 480
        ypos 270
    with dissolve
    pause(1)
    hide logo
    with dissolve
    return

label start:
    call memory_0
    call chapter_trans
    call chapter_1
    call chapter_trans
    call chapter_2
    call chapter_trans
    call chapter_3
    call chapter_trans
    return
